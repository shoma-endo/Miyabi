  "description": "Business Strategy & Marketing Agents for Miyabi - 14 specialized agents for business operations",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
